    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "cross-env": "6.0.3",
    "eslint": "6.7.2",
    "eslint-config-standard": "11.0.0",
    "eslint-config-standard-react": "9.2.0",
